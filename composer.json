{
  "name": "rawr/covers-ignore",
  "description": "Script to remove PhpUnit @covers annotations from project",
  "type": "library",
  "license": "MIT",
  "scripts": {
    "test": "phpunit test --no-coverage"
  },
  "autoload": {
    "psr-4": {
      "Kelunik\\FqnCheck\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "test/"
    ],
    "psr-4": {
      "Test\\": "test/"
    }
  },
  "bin": [
    "bin/fqn-check"
  ],
  "require": {
    "nikic/php-parser": "^2.0|^3.0|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "mikey179/vfsstream": "^1.6"
  }
}
